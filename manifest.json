{
	"manifest_version": 3, 
	"name": "filter-tool",
	"version": "1.0.0",
	"description": "chrome page filter tools",
	"icons":
	{
		"16": "img/logo.png",
		"48": "img/logo.png",
		"128": "img/logo.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"css": ["css/filter.css"],
			"run_at": "document_start"
		}
	],
	"permissions":
	[
		"storage"
	]
}
